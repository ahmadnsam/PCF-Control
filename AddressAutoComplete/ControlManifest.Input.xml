<?xml version="1.0" encoding="utf-8" ?>
<manifest>
  <control namespace="viteapps" constructor="AddressAutoComplete" version="2.0.1" display-name-key="AddressAutoComplete" description-key="AddressAutoComplete description" control-type="standard" >
    
    <external-service-usage enabled="false">
    </external-service-usage>
    <property name="fulladdress" display-name-key="Full address" description-key="Full Address" of-type="SingleLine.Text" usage="bound" required="true"/>
    <property name="apiKey" display-name-key="API Key" description-key="Your Geoapify API Key" of-type="SingleLine.Text" usage="input" required="true"/>
    <property name="countryCodes" display-name-key="Country Codes" description-key="filter on one or multiple country code separating them by to ISO 3166-1 Alpha-2 Use auto to detect the country by IP address Use none to default value" of-type="SingleLine.Text" usage="input" required="false"/>
    <property name="address_street1" display-name-key="Street 1" description-key="Street 1" of-type="SingleLine.Text" usage="bound" required="false"/>
    <property name="address_street2" display-name-key="Street 2" description-key="Street 2" of-type="SingleLine.Text" usage="bound" required="false"/>
    <property name="address_city" display-name-key="City" description-key="City" of-type="SingleLine.Text" usage="bound" required="false"/>
    <property name="address_state" display-name-key="State/Province" description-key="State/Province" of-type="SingleLine.Text" usage="bound" required="false"/>
    <property name="address_zipcode" display-name-key="Zip Code" description-key="Zip Code" of-type="SingleLine.Text" usage="bound" required="false"/>
    <property name="address_country" display-name-key="Country" description-key="Country" of-type="SingleLine.Text" usage="bound" required="false"/>
    
    <resources>
      <code path="index.ts" order="1"/>
      <css path="styles/minimal.css" order="1" />
    </resources>
  </control>
</manifest>
